<!DOCTYPE html>
<html>
    <head>

        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <style>
            body {
                font-family: Arial, sans-serif;
                margin: 0;
                padding: 0;
                background-color: #f6f9ff;
            }

            h1 {
                text-align: center;
                color: #333;
            }

            div strong {
                font-weight: bold;
            }

            div.result {
                background-color: #4caf50;
                color: #fff;
                padding: 10px;
            }

            div.error {
                background-color: #ff5722;
                color: #fff;
                padding: 10px;
            }

        </style>

    </head>
    <body>

        <div>
            <a href="https://localhost:7111/User/User/UserCompiler" class="btn btn-primary">
                Back
            </a>

        <!--<h1>COMPILER OUTPUT  PAGE.</h1>-->

        </div>



        @if (!string.IsNullOrEmpty(ViewBag.Result))
        {
            <br />
            <div class="result">
                <strong>Output: </strong> @ViewBag.Result
            </div>
        }

        @if (!string.IsNullOrEmpty(ViewBag.Error))
        {
            <br />
            <div class="error">
                <strong>Error:</strong> @ViewBag.Error
            </div>
        }

    </body>
</html>